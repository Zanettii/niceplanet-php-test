                                        },
                                        "nomePropriedade": {
                                            "type": "string",
                                            "example": "Fazenda São João"
                                        },
                                        "cpfPropriedade": {
                                            "type": "string",
                                            "example": "123.456.789-01"
                                        },
                                        "isActive": {
                                            "type": "boolean",
                                            "example": true
                                        },
                                        "created_at": {
                                            "type": "string",
                                            "format": "date-time",
                                            "example": "2024-01-01T00:00:00Z"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "400": {
                        "description": "Dados inválidos fornecidos"
                    },
                    "500": {
                        "description": "Erro interno no servidor"
                    }
                },
                "security": [
                    {
                        "Bearer": []
                    }
                ]
            }
        },
        "/api/propriedade/{id}": {
            "get": {
                "tags": [
                    "Propriedade"
                ],
                "summary": "Obter detalhes de uma propriedade",
                "description": "Obtém os detalhes de uma propriedade com base no ID fornecido.",
                "operationId": "showPropriedade",
                "parameters": [
                    {
                        "name": "id",
                        "in": "path",
                        "description": "ID da propriedade a ser obtida",
                        "required": true,
                        "schema": {
                            "type": "string",
                            "format": "uuid"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Detalhes da propriedade obtidos com sucesso",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "idPropriedade": {
                                            "type": "integer",
                                            "example": 1
                                        },
                                        "nomePropriedade": {
                                            "type": "string",
                                            "example": "Fazenda São João"
                                        },
                                        "cadastroRural": {
                                            "type": "string",
                                            "example": "1234567890"
                                        },
                                        "isActive": {
                                            "type": "boolean",
                                            "example": true
                                        },
                                        "created_at": {
                                            "type": "string",
                                            "format": "date-time",
                                            "example": "2024-01-01T00:00:00Z"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "404": {
                        "description": "Propriedade não encontrada"
                    },
                    "500": {
                        "description": "Erro interno no servidor"
                    }
                },
                "security": [
                    {
                        "Bearer": []
                    }
                ]
            }
        }
    },
    "components": {
        "securitySchemes": {
